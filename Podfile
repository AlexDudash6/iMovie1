  pod ‘OXPatternLock’
  pod ‘CDAlertView’
  pod ‘lottie-ios’
  pod 'Alamofire', '~> 4.7'
  pod 'AlamofireObjectMapper', :git => 'https://github.com/tristanhimmelman/AlamofireObjectMapper.git', :branch => 'swift-4' 
  pod ’SimplePDF’
  pod ‘CircleMenu’
  pod ‘CSPieChart’
 
end
